<JanuaryCreateWindow>:
    name: "Januarycreate"

    namee: namee
    email: email
    password: passw
    nameFeb: January

    FloatLayout:
        cols:1

        FloatLayout:
            size: root.width, root.height/2

            Label:
                text: "G000000000000000000000000000D"
                size_hint: 0.8, 0.2
                pos_hint: {"x":0.1, "top":1}
                font_size: (root.width**2 + root.height**2) / 14**4



            TextInput:
                pos_hint: {"x":0.5, "top":0.8}
